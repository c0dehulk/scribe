{
    "name": "codehulk/scribe",
    "description": "A documentation generator for packages.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Piper",
            "email": "id+git@codehulk.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "bin": [
        "bin/scribe.php"
    ],
    "autoload": {
        "psr-4": {
            "Codehulk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codehulk\\": "tests/unit/"
        }
    },
    "require": {
        "php": "^7.0",
        "symfony/console": "^2.0",
        "codehulk/package-tools": "^0.1",
        "twig/twig": "^2.0",
        "php-di/php-di": "^6.0",
        "symfony/filesystem": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
